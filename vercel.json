{
	"version": 2,
	"builds": [{ "src": "./index.js", "use": "@vercel/node" }],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/",
			"methods": ["GET", "POST", "PUT", "DELETE"]
		}
	],
    "functions": {
    "controller/test.js": {
      "maxDuration": 30 
    },
    "controller/*.js": {
      "maxDuration": 55
    }
  }

}
